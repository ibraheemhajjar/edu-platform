{
  "name": "medusa",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/medusa/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "apps/medusa"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "apps/medusa"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm start",
        "cwd": "apps/medusa"
      }
    }
  },
  "tags": ["type:app"]
}
